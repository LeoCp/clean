{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "typeRoots": ["node_modules/@types", "./src/types"],
    "paths": {
      "@domain/*": ["./src/domain/*"],
      "@assets/*": ["./src/assets/*"],
      "@util/*": ["./src/util/*"],
      "@routes/*": ["./src/routes/*"],
      "@components/*": ["./src/components/*"],
      "@theme/*": ["./src/theme/*"],
      "@constants/*": ["src/constants/*"],
      
    }                              
  }
}
